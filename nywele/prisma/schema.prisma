// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String   @id @default(uuid())
  title       String
  description String
  price       Float
  variants    Json
  date        DateTime @default(now())
}

model product {
  id          String   @id @default(uuid())
  title       String
  description String
  price       Float
  variants    Json
  date        DateTime @default(now())
}
